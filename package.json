{
  "name": "course-player",
  "version": "1.0.0",
  "description": "An online course presentation engine with React.",
  "main": "index.js",
  "scripts": {
    "start": "serve static",
    "build": "browserify source/index.js -o static/index.js -t [ babelify --presets [ es2015 react stage-2 ] ]",
    "lint": "eslint .",
    "test": "NODE_PATH=source babel-node source/test/index.js",
    "watch": "watch 'clear && npm run -s test && npm run -s build' source"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jshomes/code-exercise.git"
  },
  "author": "Eric Elliott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jshomes/code-exercise/issues"
  },
  "homepage": "https://github.com/jshomes/code-exercise#readme",
  "dependencies": {
    "brace": "0.7.0",
    "browserify": "13.0.0",
    "react": "0.14.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "enzyme": "2.1.0",
    "estraverse-fb": "1.3.1",
    "react-addons-test-utils": "0.14.7",
    "serve": "1.4.0",
    "tape": "4.5.1",
    "watch": "0.17.1"
  }
}
